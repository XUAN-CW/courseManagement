<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.coursemanagement.student.ui.homework.HomeFragment"
        android:label="@string/student_title_home"
        tools:layout="@layout/student_main" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.coursemanagement.student.ui.dashboard.DashboardFragment"
        android:label="@string/student_title_dashboard"
        tools:layout="@layout/student_fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.coursemanagement.student.ui.notifications.NotificationsFragment"
        android:label="@string/student_title_notifications"
        tools:layout="@layout/student_fragment_notifications" />
</navigation>
